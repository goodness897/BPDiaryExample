<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <size android:width="154dp" android:height="46dp" />
            <stroke android:width="1dp" android:color="#555053" />
            <corners android:bottomRightRadius="100dp" android:topRightRadius="100dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <size android:width="154dp" android:height="46dp" />
            <stroke android:width="1dp" android:color="#d7d5d6" />
            <corners android:bottomRightRadius="100dp" android:topRightRadius="100dp" />
        </shape>
    </item>
</selector>